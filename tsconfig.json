{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@contracts/*": [
        "src/domain/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "include": [
    "src"
  ]
}